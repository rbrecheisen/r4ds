    "neoadjuvant_castor_10": {
        "description": "Neoadjuvant treatment",
        "data_type": "factor",
        "values": {
            "NA": "Unknown",
            "0": "No",
            "1": "Chemoradiotherapy",
            "2": "Chemotherapy",
            "3": "Radiotherapy",
            "7": "Other"
        }
    },
    "neoadj_treatment_other": {
        "description": "Description of other neoadjuvant treatment",
        "data_type": "string",
        "depends_on": "neoadjuvant_castor_10"
    },
    "neoadjuvant_chemo": {
        "description": "Type of chemotherapy",
        "data_type": "factor",
        "values": {
            "1": "FOLFIRINOX",
            "2": "Gemcitabine",
            "7": "Other"
        },
        "depends_on": "neoadjuvant_castor_10 in [1,2]"
    },
    "neoadj_anders" :{
        "description": "Description of other type of chemotherapy",
        "data_type": "string",
        "depends_on": "neoadjuvant_chemo == 7"
    },
    "sandostatine": {
        "description": "Somatostatin analogues",
        "data_type": "factor",
        "values": {
            "NA": "No",
            "1": "Lanreotide (somatulin)",
            "2": "Ocreotide",
            "3": "Sandostatin",
            "4": "Sandostatin LAR",
            "5": "Pasireotide (signifor)",
            "6": "Somatostatin",
            "7": "Unknown",
            "77": "Other, namely"
        }
    },
    "sandostat_anders": {
        "description": "Description of other somatostatin analogue",
        "data_type": "string",
        "depends_on": "sandostatine == 77"
    },
    "implementphase": {
        "description": "Phase in stepped-wedge implementation study",
        "data_type": "factor",
        "values": {
            "1": "Pre-implementation (Baseline period before the new protocol is used. Usual care only; this is your control phase)",
            "2": "Wash-in (Transition period where the new protocol is being introduced but not yet consistently applied. Staff training, protocol rollout, and gradual uptake happen here. Data from this phase are often excluded or analyzed separately because adherence is inconsistent)",
            "3": "Post-implementation (The new protocol is fully in place and routinely followed. This is the “after” phase for outcome comparison)"
        }
    },
    "typok": {
        "description": "Type of resection",
        "data_type": "factor",
